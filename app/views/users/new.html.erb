<h1>New User Form</h1>

<%= form_for @user do |form| %>
  <%= form.label :username %>
  <%= form.text_field :username %>

  <%= form.label :password %>
  <%= form.text_field :password %>

  <%= form.label :password_confirmation, "Password Confirmation" %>
  <%= form.text_field :password_confirmation %>

  <%= form.submit "Sign Up" %>
<% end %>
